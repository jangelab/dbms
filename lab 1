CREATE TABLE STUDIO_DETAILS
(
	SNAME VARCHAR2(20),
	BRANCH VARCHAR2(20),
	CONSTRAINT STUDIO_PK PRIMARY KEY(SNAME)
);

CREATE TABLE STUDIO_LOCATION
(
	SNAME VARCHAR2(20),
	LOCATION VARCHAR2(20),
	CONSTRAINT LOC_FK FOREIGN KEY(SNAME) REFERENCES STUDIO_DETAILS(SNAME)
);

CREATE TABLE CREW
(
	SNAME VARCHAR2(20),
	CNO NUMBER(3),
	SECTOR VARCHAR2(20),
	STRENGTH NUMBER(3),
	CONSTRAINT CREW_FK FOREIGN KEY(SNAME) REFERENCES STUDIO_DETAILS(SNAME)
);


INSERT INTO STUDIO_DETAILS VALUES('&SNAME','&BRANCH');

INSERT INTO STUDIO_LOCATION VALUES('&SNAME','&LOCATION');

INSERT INTO CREW VALUES('&SNAME',&CNO,'&SECTOR',&STRENGTH);

SELECT * FROM STUDIO_DETAILS;

SELECT * FROM STUDIO_LOCATION;

SELECT * FROM CREW;

SELECT SNAME
FROM STUDIO_DETAILS
WHERE SNAME NOT IN (SELECT SNAME FROM CREW);

SELECT SNAME
FROM CREW 
WHERE STRENGTH>=ALL(SELECT STRENGTH FROM CREW);

