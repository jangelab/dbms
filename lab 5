create table Owner_of_dog(name varchar(20), ssn varchar(20) primary key, address varchar(20));
create table Dog(ssn varchar(10), d_name varchar(10), primary key(ssn,d_name), gender varchar (20),foreign key(ssn) references Owner_of_dog(ssn),  dop DATE);

