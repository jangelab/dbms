select * from Dog;

        SELECT D.d_name,D.dop,D.gender,O.name 
	FROM Dog D,owner_of_dog O
	WHERE (O.ssn = D.ssn AND O.name ='binod');

       

        SELECT O.name 
	FROM Owner_of_dog O
	WHERE O.ssn NOT IN(
		SELECT D.ssn FROM dog D,Owner_of_dog O
		WHERE (O.ssn = D.ssn)
		);

