CREATE TABLE studioo
(
	st_name varchar(20),
	branch varchar(20),
	location varchar(20),
	constraint studioo_pk PRIMARY KEY(st_name)
);

CREATE TABLE cartoon
(
	title varchar2(20),
	sensor_no varchar2(20),
	yop number(5),
	constraint cartoon_pk PRIMARY KEY(sensor_no)
);

CREATE TABLE staar
(
	name varchar2(20),
	address varchar2(20),
	constraint staar_pk PRIMARY KEY(name)
);

CREATE TABLE owned_by
(
	st_name varchar2(20),
	sensor_no varchar2(20),
	constraint fk1 FOREIGN KEY(st_name) REFERENCES studioo(st_name),
	constraint fk2 FOREIGN KEY(sensor_no) REFERENCES cartoon(sensor_no)
);

CREATE TABLE voice_by
(
	name varchar2(20),	
	sensor_no varchar2(20),
	constraint fk3 FOREIGN KEY(name) REFERENCES staar(name),
	constraint fk4 FOREIGN KEY(sensor_no) REFERENCES cartoon(sensor_no)
);

INSERT INTO studioo VALUES ('&st_name','&branch','&location');

INSERT INTO cartoon VALUES ('&title','&sensor_no',&yop);

INSERT INTO staar VALUES ('&name','&address');

INSERT INTO owned_by VALUES ('&st_name','&sensor_no');

INSERT INTO voice_by VALUES ('&name','&sensor_no');

SELECT * FROM studioo;

SELECT * FROM cartoon;

SELECT * FROM staar;

SELECT * FROM owned_by;

SELECT * FROM voice_by;

SELECT s.st_name,s.location,c.title
FROM studioo s,cartoon c,owned_by o,voice_by v
WHERE s.st_name=o.st_name AND c.sensor_no=o.sensor_no AND v.name='NANI' AND v.sensor_no=c.sensor_no;

SELECT count(*)
FROM staar s,cartoon c,voice_by v
WHERE s.name=v.name AND v.sensor_no=c.sensor_no AND c.title='A';




