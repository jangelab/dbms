select * from Acted_by;

select E.st_name
from Estudio E,Movie M,owns o
where M.title='Mobius' and E.st_name=o.st_name and M.sensor_no=o.sensor;

select S.star_name
from Stars s,Acted_by A, Movie M
where M.title='Batman' and S.star_name=A.star_name and M.sensor_no=A.sensor_no;
 







