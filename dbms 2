create table Estudio(st_name varchar(20) primary key, branch varchar(20), location varchar(20));
create table Movie(sensor_no int primary key, title varchar(20), years int);
create table Stars(star_name varchar(20) primary key, address varchar(20));
create table owns(st_name varchar(20), sensor int, primary key(st_name,sensor),foreign key(st_name)references Estudio(st_name), foreign key(sensor)references Movie(sensor_no));
create table Acted_by(star_name varchar(20), sensor_no int, primary key(star_name,sensor_no),foreign key(star_name)references Stars(star_name),foreign key(sensor_no)references Movie(sensor_no));

insert into Estudio values('A','marvel','USA');
insert into Estudio values('B','marvel','Australia');
insert into Estudio values('C','marvel','UK');
insert into Estudio values('D','marvel','China');
insert into Estudio values('E','marvel','India');
select * from Estudio;

