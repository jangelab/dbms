CREATE TABLE studio
(
	st_name varchar(20),
	branch varchar(20),
	location varchar(20),
	constraint pk22 PRIMARY KEY(st_name)
);

CREATE TABLE movie
(
	sensor_no varchar(20),
	title varchar(20),
	year number(5),
	constraint pk23 PRIMARY KEY(sensor_no)
);

CREATE TABLE star
(
	star_name varchar(20),
	address varchar(20),
	constraint pk24 PRIMARY KEY(star_name)
);

CREATE TABLE owns
(
	st_name varchar(20),
	sensor_no varchar(20),
	constraint pk25 PRIMARY KEY(st_name,sensor_no),
	constraint fk21 FOREIGN KEY(st_name) REFERENCES studio(st_name),
	constraint fk22 FOREIGN KEY(sensor_no) REFERENCES movie(sensor_no)
);

CREATE TABLE acted_by
(
	star_name varchar(20),
	sensor_no varchar(20),
	constraint pk26 PRIMARY KEY(star_name,sensor_no),
	constraint fk23 FOREIGN KEY(star_name) REFERENCES star(star_name),
	constraint fk24 FOREIGN KEY(sensor_no) REFERENCES movie(sensor_no)
);

INSERT INTO studio VALUES ('&st_name','&branch','&location');

INSERT INTO movie VALUES ('&sensor_no','&title',&year);

INSERT INTO star VALUES ('&star_name','&address');

INSERT INTO owns VALUES ('&st_name','&sensor_no');

INSERT INTO acted_by VALUES ('&star_name','&sensor_no');

SELECT * FROM studio;

SELECT * FROM movie;

SELECT * FROM star;

SELECT * FROM owns;

SELECT * FROM acted_by;

SELECT m.title,s.st_name,s.branch,s.location
FROM studio s,movie m,owns o
WHERE s.st_name=o.st_name AND m.sensor_no=o.sensor_no AND m.title='one';

SELECT m.title,s.star_name,s.address
FROM star s,movie m,acted_by a
WHERE s.star_name=a.star_name AND m.sensor_no=a.sensor_no AND m.title='two';




